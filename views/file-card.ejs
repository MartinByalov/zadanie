<div class="file-card" data-name="<%= file.name %>">
  <div class="file-icon"><%- include('file-icon', { file }) %></div>
  <div class="file-name">
    <%= file.name %>
  </div>
  <div class="file-modified">
    <%= new Date(file.modifiedTime).toLocaleString() %>
  </div>
  <a href="<%= file.webViewLink %>" target="_blank" rel="noopener noreferrer">Отвори</a>
</div>
